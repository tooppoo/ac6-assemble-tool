{
  "name": "@ac6_assemble_tool/parts",
  "version": "1.6.1",
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/*.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "imports": {
    "~parts/*": "./dist/*"
  },
  "scripts": {
    "dev": "npm run build && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "build": "tsc && tsc-alias",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "clean": "rimraf ./dist",
    "check-types": "tsc --noEmit"
  },
  "author": "philomagi <tooppoo56@gmail.com> (https://github.com/tooppoo)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0"
  }
}
