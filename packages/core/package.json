{
  "name": "@ac6_assemble_tool/core",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./src/*.ts",
        "default": "./dist/src/*.js"
      }
    },
    "./spec-helper/*": {
      "import": {
        "types": "./spec-helper/*.ts",
        "default": "./dist/spec-helper/*.js"
      }
    }
  },
  "imports": {
    "#core/*": "./src/*.ts",
    "#spec/*": "./spec/*.ts",
    "#spec-helper/*": "./spec-helper/*.ts"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "check-types": "tsc --noEmit"
  },
  "author": "philomagi <tooppoo56@gmail.com> (https://github.com/tooppoo)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "1.0.0",
    "@ac6_assemble_tool/spec": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0",
    "fake-indexeddb": "6.0.0"
  },
  "dependencies": {
    "@ac6_assemble_tool/parts": "1.7.0",
    "dexie": "4.0.8",
    "ulid": "2.3.0",
    "zod": "3.23.8"
  }
}
