{
  "name": "@ac6_assemble_tool/core",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/src/*.ts",
        "default": "./dist/src/*.js"
      }
    },
    "./spec-helper/*": {
      "import": {
        "types": "./dist/spec-helper/*.ts",
        "default": "./dist/spec-helper/*.js"
      }
    }
  },
  "scripts": {
    "dev": "tsc && (concurrently \\\"tsc -w\\\" \\\"tsc-alias -w\\\")",
    "build": "tsc && tsc-alias",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "clean": "rimraf ./dist",
    "check-types": "tsc --noEmit"
  },
  "author": "philomagi <tooppoo56@gmail.com> (https://github.com/tooppoo)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@ac6_assemble_tool/spec": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0",
    "fake-indexeddb": "6.0.0"
  },
  "dependencies": {
    "@ac6_assemble_tool/parts": "1.6.1",
    "dexie": "4.0.8",
    "ulid": "2.3.0",
    "zod": "3.23.8"
  }
}
