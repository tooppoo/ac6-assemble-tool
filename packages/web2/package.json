{
	"name": "@ac6_assemble_tool/web2",
	"version": "1.16.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
		"format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
		"test:integration": "playwright test",
		"test:all": "npm run test:integration && npm run test:unit"
	},
	"devDependencies": {
		"@playwright/test": "1.28.1",
		"@sveltejs/adapter-static": "3.0.5",
		"@sveltejs/kit": "2.5.28",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"eslint-plugin-svelte": "2.44.0",
		"globals": "15.9.0",
		"svelte-check": "4.0.2"
	},
	"dependencies": {
		"@ac6_assemble_tool/core": "1.0.0",
		"@ac6_assemble_tool/parts": "1.7.0",
		"bootstrap": "5.3.3",
		"bootstrap-icons": "1.11.3",
		"i18next": "23.15.1",
		"svelte": "4.2.19",
		"svelte-i18next": "2.2.2"
	},
	"type": "module"
}
