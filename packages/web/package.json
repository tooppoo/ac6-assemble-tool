{
  "name": "@ac6_assemble_tool/web",
  "version": "1.16.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "postbuild": "npm run sitemap && npm run purgecss",
    "prepreview": "npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check-types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check-types:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "purgecss": "purgecss --config purgecss.config.cjs",
    "sitemap": "svelte-sitemap --domain https://tooppoo.github.io/ac6_assemble_tool -o dist -r -c weekly",
    "test:integration": "playwright test",
    "test:all": "npm run test:integration && npm run test:unit"
  },
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "1.0.0",
    "@ac6_assemble_tool/spec": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0",
    "@fullhuman/postcss-purgecss": "6.0.0",
    "@playwright/test": "1.28.1",
    "@sveltejs/adapter-static": "3.0.5",
    "@sveltejs/kit": "2.5.28",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@sveltestrap/sveltestrap": "6.2.7",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/svelte": "5.2.3",
    "eslint-plugin-svelte": "2.44.1",
    "fake-indexeddb": "6.0.0",
    "globals": "15.10.0",
    "jsdom": "25.0.1",
    "sass-embedded": "1.79.4",
    "svelte-check": "4.0.2",
    "svelte-sitemap": "2.6.0",
    "vite-bundle-analyzer": "0.11.1"
  },
  "dependencies": {
    "@ac6_assemble_tool/core": "1.0.0",
    "@ac6_assemble_tool/parts": "1.7.0",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "i18next": "23.15.1",
    "svelte": "4.2.19",
    "svelte-i18next": "2.2.2"
  },
  "type": "module"
}
