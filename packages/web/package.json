{
  "name": "@ac6_assemble_tool/web",
  "private": true,
  "version": "1.16.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check-types && vite build",
    "build-only": "vite build",
    "analyze": "ANALIZE_MODE=static vite build",
    "analyze:open": "ANALYZE_MODE=server vite build",
    "preview": "vite preview",
    "test": "VITE_LOG_LEVEL=silent vitest run",
    "test:watch": "VITE_LOG_LEVEL=silent vitest",
    "coverage": "VITE_LOG_LEVEL=silent vitest run --coverage",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "check-types": "tsc && svelte-check --tsconfig ./tsconfig.json",
    "extract-ja-chars": "tsx ./scripts/extract-ja-characters/extract-from-i18n.ts",
    "clean": "rimraf ./dist"
  },
  "author": "philomagi <tooppoo56@gmail.com> (https://github.com/tooppoo)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@ac6_assemble_tool/eslint": "1.0.0",
    "@ac6_assemble_tool/spec": "1.0.0",
    "@ac6_assemble_tool/tsconfig": "1.0.0",
    "@popperjs/core": "2.11.8",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/svelte": "5.2.1",
    "@tsconfig/svelte": "5.0.4",
    "@types/bootstrap": "5.2.10",
    "eslint-plugin-svelte": "2.44.1",
    "fake-indexeddb": "6.0.0",
    "globals": "15.10.0",
    "jsdom": "25.0.1",
    "sass": "1.79.4",
    "svelte-check": "4.0.4",
    "svelte-eslint-parser": "0.41.1",
    "vite-bundle-analyzer": "0.12.1",
    "vite-plugin-dynamic-import": "1.6.0",
    "vite-plugin-purgecss-updated-v5": "1.2.1",
    "vite-plugin-sitemap": "0.7.1"
  },
  "dependencies": {
    "@ac6_assemble_tool/core": "1.0.0",
    "@ac6_assemble_tool/parts": "1.7.0",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "i18next": "23.15.1",
    "svelte": "4.2.19",
    "svelte-i18next": "2.2.2"
  }
}
