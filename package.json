{
  "name": "ac6_assemble_tool",
  "private": true,
  "version": "1.12.0",
  "type": "module",
  "engines": {
    "node": ">=21.6.1",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run check",
    "build:ci": "tsc && vite build",
    "preview": "vite preview",
    "test": "VITE_LOG_LEVEL=silent vitest run",
    "test:watch": "VITE_LOG_LEVEL=silent vitest",
    "coverage": "VITE_LOG_LEVEL=silent vitest run --coverage",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.7.0",
    "@fast-check/vitest": "0.1.2",
    "@popperjs/core": "2.11.8",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/svelte": "5.2.0",
    "@tsconfig/svelte": "5.0.4",
    "@types/bootstrap": "5.2.10",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.14.10",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-svelte": "2.42.0",
    "fake-indexeddb": "6.0.0",
    "fast-check": "3.20.0",
    "globals": "15.8.0",
    "jsdom": "24.1.0",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "sinon": "18.0.0",
    "svelte-check": "3.8.4",
    "svelte-eslint-parser": "0.40.0",
    "tslib": "2.6.3",
    "typescript": "5.5.3",
    "typescript-eslint": "7.16.0",
    "vite": "5.3.3",
    "vite-plugin-dynamic-import": "1.5.0",
    "vite-plugin-sitemap": "^0.6.0",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "dexie": "4.0.8",
    "i18next": "23.12.1",
    "lodash-es": "4.17.21",
    "loglevel": "1.9.1",
    "svelte": "4.2.18",
    "svelte-i18next": "2.2.2",
    "ulid": "2.3.0",
    "zod": "3.23.8"
  }
}
