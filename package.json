{
  "name": "ac6_assemble_tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=21.6.1",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "VITE_LOG_LEVEL=silent vitest run",
    "test:watch": "VITE_LOG_LEVEL=silent vitest",
    "coverage": "VITE_LOG_LEVEL=silent vitest run --coverage",
    "lint": "eslint . && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.1",
    "@eslint/js": "8.57.0",
    "@fast-check/vitest": "0.1.0",
    "@popperjs/core": "2.11.8",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/svelte": "4.1.0",
    "@tsconfig/svelte": "5.0.2",
    "@types/node": "20.11.28",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitest/coverage-v8": "1.4.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "fast-check": "3.16.0",
    "globals": "14.0.0",
    "jsdom": "24.0.0",
    "prettier": "3.2.5",
    "sass": "1.72.0",
    "sinon": "17.0.1",
    "svelte": "4.2.12",
    "svelte-check": "3.6.7",
    "svelte-eslint-parser": "0.33.1",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "typescript-eslint": "7.2.0",
    "vite": "5.1.6",
    "vite-plugin-dynamic-import": "1.5.0",
    "vitest": "1.4.0"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "loglevel": "1.9.1"
  }
}
