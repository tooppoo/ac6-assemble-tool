{
  "name": "ac6_assemble_tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=21.6.1",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write \"**/*.+(js|json|ts|yml|yaml|html)\"",
    "format:dry": "prettier . --check \"**/*.+(js|json|ts|yml|yaml|html)\"",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.1",
    "@fast-check/vitest": "0.1.0",
    "@popperjs/core": "2.11.8",
    "@sveltejs/vite-plugin-svelte": "3.0.2",
    "@tsconfig/svelte": "5.0.2",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitest/coverage-v8": "1.3.0",
    "bootstrap": "5.3.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "fast-check": "3.15.1",
    "prettier": "3.2.5",
    "sass": "1.71.0",
    "svelte": "4.2.11",
    "svelte-check": "3.6.4",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "typescript-eslint": "7.0.1",
    "vite": "5.1.3",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "eslint-plugin-svelte": "^2.35.1"
  }
}
