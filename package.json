{
  "name": "ac6_assemble_tool",
  "private": true,
  "version": "1.14.2",
  "type": "module",
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "build-only": "vite build",
    "analyze": "ANALIZE_MODE=static vite build",
    "analyze:open": "ANALYZE_MODE=server vite build",
    "preview": "vite preview",
    "test": "VITE_LOG_LEVEL=silent vitest run",
    "test:watch": "VITE_LOG_LEVEL=silent vitest",
    "coverage": "VITE_LOG_LEVEL=silent vitest run --coverage",
    "lint": "eslint . && prettier . --check \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "format": "eslint . --fix && prettier . --write \"**/*.+(js|json|ts|yml|yaml|html|svelte)\"",
    "check": "tsc && svelte-check --tsconfig ./tsconfig.json",
    "extract-ja-chars": "tsx scripts/extract-ja-characters/extract-from-i18n.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@fast-check/vitest": "0.1.3",
    "@popperjs/core": "2.11.8",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/svelte": "5.2.1",
    "@tsconfig/svelte": "5.0.4",
    "@types/bootstrap": "5.2.10",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.16.5",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/coverage-v8": "2.0.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-svelte": "2.43.0",
    "fake-indexeddb": "6.0.0",
    "fast-check": "3.22.0",
    "globals": "15.9.0",
    "jsdom": "25.0.0",
    "prettier": "3.3.3",
    "sass": "1.78.0",
    "sinon": "18.0.0",
    "svelte-check": "3.8.6",
    "svelte-eslint-parser": "0.41.0",
    "tslib": "2.7.0",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "7.18.0",
    "vite": "5.4.3",
    "vite-bundle-analyzer": "0.10.6",
    "vite-plugin-dynamic-import": "1.5.0",
    "vite-plugin-purgecss-updated-v5": "1.2.1",
    "vite-plugin-sitemap": "^0.7.0",
    "vitest": "2.0.5",
    "zx": "8.1.5"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "dexie": "4.0.8",
    "i18next": "23.15.0",
    "lodash-es": "4.17.21",
    "loglevel": "1.9.2",
    "svelte": "4.2.19",
    "svelte-i18next": "2.2.2",
    "ulid": "2.3.0",
    "zod": "3.23.8"
  }
}
