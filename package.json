{
  "name": "ac6_assemble_tool",
  "private": true,
  "version": "1.14.3",
  "type": "module",
  "engines": {
    "node": "20.17.0",
    "npm": "10.8.2"
  },
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build-only": "vite build",
    "analyze": "ANALIZE_MODE=static vite build",
    "analyze:open": "ANALYZE_MODE=server vite build",
    "preview": "vite preview",
    "test": "VITE_LOG_LEVEL=silent vitest run",
    "test:watch": "VITE_LOG_LEVEL=silent vitest",
    "coverage": "VITE_LOG_LEVEL=silent vitest run --coverage",
    "lint": "turbo lint",
    "format": "turbo format",
    "check-types": "tsc && svelte-check --tsconfig ./tsconfig.json",
    "extract-ja-chars": "tsx scripts/extract-ja-characters/extract-from-i18n.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@fast-check/vitest": "0.1.3",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.16.5",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "8.5.0",
    "@typescript-eslint/parser": "8.5.0",
    "@vitest/coverage-v8": "2.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.30.0",
    "fast-check": "3.22.0",
    "prettier": "3.3.3",
    "sinon": "19.0.2",
    "tslib": "2.7.0",
    "tsx": "4.19.1",
    "turbo": "2.1.2",
    "typescript": "5.6.2",
    "typescript-eslint": "8.5.0",
    "vite": "5.4.5",
    "vitest": "2.1.1",
    "zx": "8.1.6"
  },
  "dependencies": {
    "lodash-es": "4.17.21",
    "loglevel": "1.9.2"
  },
  "workspaces": [
    "packages/parts",
    "packages/tsconfig",
    "packages/core",
    "packages/web"
  ]
}
